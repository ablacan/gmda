{   "device": "cuda:2",
        "data_processing":
                {
                "name": "tcga",
                "density": 0.5,
                "quantile_transform": true
                },
    "model": {
            "device": "cuda:2",
            "latent_dim": 32,
            "x_dim": 978,
            "output_dim": 978,
            "labels_dim": 24,
            "hidden_dims": [512,1024, 1024],
            "activation":"leaky_relu",
            "embed_cat": false
    },
    "training": {
            "dataset": "tcga",
            "nb_nn_for_prec_recall": [10],
            "activation": "leaky_relu",
            "negative_slope": 0.05,
            "optimizer": "adam",
            "lr_g": 0.0005,
            "lr_decay": false,
            "batch_size": 2048,
            "epochs": 2500,
            "bn": true,
            "checkpoint_dir": "checkpoints",
            "log_dir": "logs/",
            "fig_dir": "figures",
            "save_final_weights":true,
            "K": 800,
            "nb_dim_to_keep": 3,
            "is_dark_bin":1,
            "density": 0.5,
            "lambda": 5,
            "is_label_cat": true,
            "indic_func": "sigmoid",
            "sampling": "dbs",
            "eta":0.2,
            "loss_type":"maxabs",
            "track_precision_recall":false,
            "track_corr_error": false,
            "epochs_checkpoints": [],
            "step": 6000,
            "plot_generated_data":false,
            "plot_sampling": false
    }
}