{   "device": "cuda:0",
        "data_processing":
                {
                "name": "circles",
                "density": 0.05,
                "quantile_transform": false
                },
    "model": {
            "device": "cuda:0",
            "latent_dim": 8,
            "x_dim": 2,
            "output_dim": 2,
            "labels_dim": 1,
            "hidden_dims": [64,64],
            "activation":"leaky_relu",
            "embed_cat": false
    },
    "training": {
            "dataset": "circles_2d",
            "nb_nn_for_prec_recall": [2],
            "activation": "leaky_relu",
            "negative_slope": 0.05,
            "optimizer": "adam",
            "lr_g": 0.001,
            "lr_decay": false,
            "batch_size": 2048,
            "epochs": 150,
            "bn": true,
            "checkpoint_dir": "checkpoints",
            "log_dir": "logs/",
            "fig_dir": "figures",
            "save_final_weights":false,
            "K": 500,
            "nb_dim_to_keep": 2,
            "is_dark_bin":0.1,
            "density": 0.05,
            "lambda": 2,
            "is_label_cat": true,
            "indic_func": "sigmoid",
            "sampling": "dbs",
            "eta":0.1,
            "loss_type":"maxabs",
            "track_precision_recall":true,
            "track_corr_error": true,
            "epochs_checkpoints": [5, 20, 50, 75, 100, 150],
            "step": 25,
            "plot_generated_data":true,
            "plot_sampling": true
    }
}