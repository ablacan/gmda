{   "device": "cpu",
"data_processing":
                {
                "name": "gesture",
                "density": 0.25,
                "quantile_transform": false
                },
    "model": {
            "device": "cpu",
            "latent_dim": 16,
            "x_dim": 32,
            "output_dim": 32,
            "labels_dim": 5,
            "hidden_dims": [256,1024],
            "activation":"leaky_relu",
            "embed_cat": false
    },
    "training": {
            "dataset": "gesture",
            "nb_nn_for_prec_recall": [5],
            "activation": "leaky_relu",
            "negative_slope": 0.05,
            "optimizer": "adam",
            "lr_g": 0.001,
            "lr_decay": false,
            "batch_size": 1024,
            "epochs": 2500,
            "bn": true,
            "checkpoint_dir": "checkpoints",
            "log_dir": "logs/",
            "fig_dir": "figures",
            "save_final_weights":false,
            "K": 500,
            "nb_dim_to_keep": 3,
            "is_dark_bin":0,
            "density": 0.25,
            "lambda": 10,
            "is_label_cat": true,
            "indic_func": "sigmoid",
            "sampling": "dbs",
            "eta":0.1,
            "loss_type":"maxabs",
            "track_precision_recall":true,
            "track_corr_error": true,
            "epochs_checkpoints": [1, 500, 1000, 2000, 2500],
            "step": 1000,
            "plot_generated_data":true,
            "plot_sampling": true
    }
}