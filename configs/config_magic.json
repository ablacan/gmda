{   "device": "cuda:2",
"data_processing":
                {
                "name": "magic",
                "density": 0.25,
                "quantile_transform": false
                },
    "model": {
            "device": "cuda:2",
            "latent_dim": 8,
            "x_dim": 10,
            "output_dim": 10,
            "labels_dim": 1,
            "hidden_dims": [256,256],
            "activation":"leaky_relu",
            "embed_cat": false
    },
    "training": {
            "dataset": "magic",
            "nb_nn_for_prec_recall": [5],
            "activation": "leaky_relu",
            "negative_slope": 0.05,
            "optimizer": "rms_prop",
            "lr_g": 0.0005,
            "lr_decay": false,
            "batch_size": 1024,
            "epochs": 1500,
            "bn": true,
            "checkpoint_dir": "checkpoints",
            "log_dir": "logs/",
            "fig_dir": "figures",
            "save_final_weights":false,
            "K": 500,
            "nb_dim_to_keep": 3,
            "is_dark_bin":0.1,
            "density": 0.25,
            "lambda": 5,
            "is_label_cat": true,
            "indic_func": "sigmoid",
            "sampling": "dbs",
            "eta":0,
            "loss_type":"maxabs",
            "track_precision_recall":true,
            "track_corr_error": true,
            "epochs_checkpoints": [100, 500, 1000, 1500, 2000],
            "step": 500,
            "plot_generated_data":true,
            "plot_sampling": true
    }
}